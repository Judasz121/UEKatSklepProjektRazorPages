@page
@using Dapper
@using System.Data
@using BasiaProjektRazorPages.DBModels
@model BasiaProjektRazorPages.Pages.Account.LoginModel
@{




}

<form method="post">
  <div class="mb-3">
    <label for="userMailOrName" class="form-label">Email address</label>
    <input type="text" class="form-control" id="userMailOrName" aria-describedby="emailHelp" asp-for="userNameOrMail">
  </div>
  <div class="mb-3">
    <label class="form-label">Password</label>
    <input type="password" class="form-control" asp-for="password">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>